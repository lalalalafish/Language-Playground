{
  "languages": {
    "ts": {
      "name": "typescript",
      "extension": ".ts",
      "filename_type": "kebab-case",
      "function_name_type": "camel-case",
      "execute_command": "bun {0}",
      "template": "/**\n * @fileName: {0}\n * @description 无\n * @author {1}\n * @date: {2}\n */\nexport default function {3}() {\n    console.log('Hello, Typescript!');\n}"
    },
    "js": {
      "name": "javascript",
      "extension": ".js",
      "filename_type": "kebab-case",
      "function_name_type": "camel-case",
      "execute_command": "node {0}",
      "template": "/**\n * @fileName: {0}\n * @description 无\n * @author {1}\n * @date: {2}\n */\nfunction {3}() {\n    console.log('Hello, JavaScript!');\n}\nmodule.exports = {3};"
    },
    "ja": {
      "name": "java",
      "extension": ".java",
      "filename_type": "pascal-case",
      "function_name_type": "pascal-case",
      "execute_command": "javac {0} && java {3}",
      "template": "/**\n * @fileName: {0}\n * @description 无\n * @author {1}\n * @date: {2}\n */\npublic class {3} {\n    public static void main(String[] args) {\n        System.out.println(\"Hello, Java!\");\n    }\n}"
    },
    "py": {
      "name": "python",
      "extension": ".py",
      "filename_type": "snake-case",
      "function_name_type": "snake-case",
      "execute_command": "python {0}",
      "template": "\"\"\"\n@fileName: {0}\n@description: 无\n@author: {1}\n@date: {2}\n\"\"\"\ndef {3}():\n    print('Hello, Python!')\n\nif __name__ == '__main__':\n    {3}()"
    },
    "rs": {
      "name": "rust",
      "extension": ".rs",
      "filename_type": "snake-case",
      "function_name_type": "snake-case",
      "execute_command": "rustc {0} && .\\{4}",
      "template": "/// @fileName: {0}\n/// @description: 无\n/// @author: {1}\n/// @date: {2}\nfn main() {\n    println!(\"Hello, Rust!\");\n}"
    },
    "ps": {
      "name": "powershell",
      "extension": ".ps1",
      "filename_type": "verb-noun",
      "function_name_type": "verb-noun",
      "execute_command": "pwsh {0}",
      "template": "# @fileName: {0}\n# @description: 无\n# @author: {1}\n# @date: {2}\nfunction {3} {\n    Write-Host 'Hello, PowerShell!'\n}\n{3}"
    }  },
  "defaults": {
    "author": "Yujie Liu"
  },  
  "messageTypes": {
    "mn": {
      "name": "note",
      "format": "NOTE: {0}",
      "color": "Gray"
    },
    "mp": {
      "name": "part",
      "format": "[{0}]: ",
      "color": "Green"
    },
    "mm": {
      "name": "module",
      "format": "========== {0} ==========",
      "color": "Green"
    },
    "ms": {
      "name": "success",
      "format": "[SUCCESS] {0}",
      "color": "Green"
    },
    "mr": {
      "name": "error",
      "format": "[ERROR] {0}",
      "color": "Red"
    },
    "mi": {
      "name": "info",
      "format": "[INFO] {0}",
      "color": "Cyan"
    },
    "md": {
      "name": "default",
      "format": "{0}",
      "color": "White"
    }
  }
}